{
    "consumer": {
        "name": "test_consumer"
    },
    "provider": {
        "name": "AmqpProvider"
    },
    "interactions": [
        {
            "description": "a V4 test message",
            "metaData": {
                "contentType": "application/json"
            },
            "contents": {
                "testParam1": "value1",
                "testParam2": "value2"
            },
            "providerState": "SomeProviderState",
            "pending": false,
            "type": "Asynchronous/Messages"
        },
        {
            "description": "a V4 test message with metadata",
            "metadata": {
                "contentType": "application/json",
                "someKey": "somestring"
            },
            "contents": {
                "testParam1": "value1",
                "testParam2": "value2"
            },
            "matchingRules": {
                "metadata": {
                    "somestring": {
                        "combine": "AND",
                        "matchers": [
                            {
                                "match": "type"
                            }
                        ]
                    }
                }
            },
            "providerState": "SomeProviderState",
            "pending": false,
            "type": "Asynchronous/Messages"
        }
    ],
    "metadata": {
        "pact-specification": {
            "version": "4.0"
        },
        "pact-jvm": {
            "version": "4.3.0"
        }
    }
